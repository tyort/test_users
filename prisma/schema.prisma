generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./users.db?socket_timeout=100&connection_limit=1"
}

model User {
  id           Int      @id @default(autoincrement())
  fullName     String
  birthYear    Int
  profession   String
  friendsCount Int
  isVisible    Boolean
  isChecked    Boolean
  order        Int
}
